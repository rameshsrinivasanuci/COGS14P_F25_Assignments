N = 20
### BEGIN SOLUTION 
pi = 0
error = list()
for j in range(N):
    term = 4/(2*j+1) 
    pi = pi+((-1)**j)*term
    error.append(np.pi - pi)
### END SOLUTION


assert np.abs(error[0]) == 4-np.pi
assert np.abs(error[4]) < 0.2
assert np.abs(error[-1]) < 0.05
assert len(error) == 20  





### BEGIN SOLUTION
plt.figure(figsize = (6,4))
N = np.arange(1,21)
plt.bar(N,error)
plt.xticks(N)
plt.xlabel('N')
plt.ylabel('error')
plt.grid(axis = 'y')
### END SOLUTION






### BEGIN SOLUTION
def f(x,s):
    f = np.exp(-x**2/s**2)
    return f
x = np.arange(-3,3.1,0.1)
f1 = f(x,1)
f2 = f(x,0.5)
plt.figure(figsize = (6,4))
plt.plot(x,f1,'r-',label = 's = 1')
plt.plot(x,f2,'b-',label = 's = 0.5')
plt.legend()
plt.xlabel('x')
plt.ylabel('f')
plt.grid()
plt.show()
### END SOLUTION




