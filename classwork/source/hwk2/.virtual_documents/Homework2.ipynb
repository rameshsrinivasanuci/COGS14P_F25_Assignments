### BEGIN SOLUTION
def circle_area(r=1):
    A = np.pi*r**2
    return A
### END SOLUTION


r = 2
Area  = circle_area(r) 
assert Area == np.pi*r**2


Area = circle_area()
assert Area == np.pi*1**2 


Area = circle_area(r=3)
assert Area == np.pi*3**2





### BEGIN SOLUTION
def cylinder_area(r,h):
    A = 2*circle_area(r)+2*np.pi*r*h
    return A
### END SOLUTION


assert cylinder_area(r=2,h=1) == 12*np.pi





### BEGIN SOLUTION
def distance(a,b):
    a = np.array(a)
    b = np.array(b)
    c = a-b
    D = np.sqrt(np.sum(c**2))
    return D
### END SOLUTION


assert distance(1,2) == 1


point1 = [2,3]
point2 = [-1,-1]
assert distance(point1,point2) == 5


point1 = np.ones(9)
point2 = 2*np.ones(9)
assert distance(np.ones(4), np.zeros(4)) == 2
